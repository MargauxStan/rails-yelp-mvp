<div class="container">
  <h1 class="my-3">Create a new Restaurant</h1>
  <%= simple_form_for @restaurant do |f| %>
    <%= f.input :name %>
    <%= f.input :category %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.submit %>
  <% end %>
  
  <%= link_to 'Back to list', restaurants_path, class:'btn btn-primary'  %>
</div>